{
  "name": "GitHub Disable Delete Branch Button",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Disable delete branch button if it's merged from \"master\" branch.",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/pull/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ],
  "icons": {
    "16": "assets/icon@16.png",
    "128": "assets/icon@128.png"
  }
}
